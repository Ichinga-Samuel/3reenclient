    "primeicons": "^4.1.0",
    "primereact": "^6.0.0",
    "raw-loader": "4.0.2",
    "react": "^17.0.1",
    "react-bootstrap": "1.4.3",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "17.0.1",
