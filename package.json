{
  "name": "3reenshop-client",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn storybook\"",
    "build": "next build",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "start": "next start -p $PORT",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "commit": "git-cz",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix --cache",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "cross-env CI=true yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "less-vars-to-js": "^1.3.0",
    "next": "10.0.1",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.6.0",
    "next-pwa": "^3.1.5",
    "next-seo": "^4.11.0",
    "polished": "4.0.3",
    "raw-loader": "^4.0.2",
    "react": "17.0.1",
    "react-bootstrap": "1.4.0",
    "react-dom": "17.0.1",
    "sass": "1.28.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-backgrounds": "6.0.28",
    "@storybook/addon-docs": "6.0.28",
    "@storybook/addon-knobs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addon-viewport": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.0.28",
    "@storybook/theming": "6.0.28",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.1",
    "@testing-library/user-event": "12.2.0",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/styled-components": "^5.1.4",
    "@types/testing-library__react": "10.2.0",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "commitizen": "4.2.2",
    "concurrently": "5.3.0",
    "cross-env": "7.0.2",
    "css-loader": "5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest-dom": "3.2.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.0",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "sass-loader": "10.0.5",
    "shipjs": "0.22.0",
    "storybook-addon-jsx": "^7.3.4",
    "style-loader": "2.0.0",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test:ci"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
