{
    "presets": ["next/babel"],
    "plugins": [
        "inline-react-svg",
        ["styled-components", { "ssr": true }],
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "@": "./src"
                },
                "extensions": [".js", ".ts", ".jsx", ".tsx"]
            }
        ],
        ["@babel/plugin-proposal-decorators", { "legacy": true }]
    ]
}
